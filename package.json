{
  "name": "api-node",
  "version": "1.0.0",
  "description": "App node.js",
  "main": "index.js",
  "author": "David <davidfaria89@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec babel-node src/server.js",
    "queue": "nodemon --exec babel-node src/queue.js",
    "dev:debug": "nodemon --exec babel-node --inspect src/server.js",
    "build": "babel src --ignore test.js --out-dir ./dist --copy-files",
    "start": "npm run build && node dist/server.js",
    "test": "NODE_ENV=test && jest --verbose=true --detectOpenHandles --runInBand --forceExit",
    "secure-mode": "NODE_ENV=test && jest --watchAll --verbose=true --detectOpenHandles --runInBand --forceExit",
    "lint": "eslint src/** test/** --fix"
  },
  "dependencies": {
    "aws-sdk": "^2.630.0",
    "base-64": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.2.3",
    "cors": "^2.8.5",
    "date-fns": "^2.10.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.21.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.2",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.4",
    "nodemailer-express-handlebars": "^3.1.0",
    "pagarme": "^4.7.0",
    "winston": "^3.2.1",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/runtime": "^7.8.4",
    "@shelf/jest-mongodb": "^1.1.3",
    "@types/jest": "^25.1.3",
    "@types/mongoose": "^5.7.3",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  }
